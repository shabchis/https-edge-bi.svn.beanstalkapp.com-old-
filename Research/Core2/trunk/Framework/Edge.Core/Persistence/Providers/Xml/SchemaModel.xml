<?xml version="1.0" encoding="utf-8" ?>
<ObjectSchema xmlns="http://eggplant/schema">

	<ObjectSet Namespace="Edge.Core.Schema.Configuration">

		<Object Name="ObjectSchema">
			<Properties>
				<Property Name="ObjectSets" Type="List[ObjectSet]"/>
			</Properties>
		</Object>

		<Object Name="ObjectSet">
			<Properties>
				<Property Name="Namespace" Type="string" AllowEmpty="false"/>
				<Property Name="Objects" Type="List[ObjectDefinition]"/>
			</Properties>
		</Object>

		<Object Name="ObjectDefinition">
			<Properties>
				<Property Name="Namespace" Type="string" AllowEmpty="true"/>
				<Property Name="Name" Type="string" AllowEmpty="false"/>
				<Property Name="IsAbstract" Type="bool" DefaultValue="false"/>
				<Property Name="BaseDefinition" Type="ObjectDefinition" AllowEmpty="true"/>
				<Property Name="Properties" Type="List[PropertyDefinition]"/>
				<Property Name="Queries" Type="List[QueryDefinition]"/>
			</Properties>
		</Object>

		<Object Name="PropertyDefinition">
			<Properties>
				<Property Name="Name" Type="string"/>
				<Property Name="TypeName" Type="string"/>
				<Property Name="Access" Type="PropertyAccess" AllowEmpty="true"/>
				<Property Name="EmptyValue" Type="object" AllowEmpty="true"/>
				<Property Name="DefaultValue" Type="object" AllowEmpty="true"/>
				<Property Name="AllowEmpty" Type="bool" DefaultValue="false"/>
				<Property Name="Constraints" Type="List[PropertyContraint]"/>
			</Properties>
		</Object>

		<Object Name="PropertyConstraint">
			
		</Object>
		
	</ObjectSet>

</ObjectSchema>
