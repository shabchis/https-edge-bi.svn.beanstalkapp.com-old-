<?xml version="1.0"?>

<!-- For more information on using web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration>
	<system.web>
		<compilation debug="true" targetFramework="4.0"/>
	</system.web>
	<system.webServer>
		<modules runAllManagedModulesForAllRequests="true">
			<add name="UrlRoutingModule" type="System.Web.Routing.UrlRoutingModule, System.Web, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
		</modules>

		<handlers>
			<clear/>
			<add name="RootHandler" verb="*" path="*" type="EdgeApiRest.Handlers.TestUriHandler,EdgeApiRest" resourceType="Unspecified" />
		</handlers>

		<!--<rewrite>
			<rules>
				<rule name="Rewrite CI Index">
					<match url=".*" />
					<conditions>
						<add input="{REQUEST_FILENAME}" pattern="css|js|jpg|jpeg|png|gif|ico|htm|html|xbap|svc|deploy|manifest|dll" negate="true" />
					</conditions>
					<action type="Rewrite" url="index.php/{R:0}" />
				</rule>
			</rules>
		</rewrite>-->
	</system.webServer>

</configuration>