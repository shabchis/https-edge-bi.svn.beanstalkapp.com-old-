<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="SmtpConnection" type="System.Configuration.SingleTagSectionHandler" />
		<section name="edge.services" type="Easynet.Edge.Core.Configuration.ServicesSection, Edge.Core" />
		<section name="GoogleCreativeReport" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="BingCreativeReport" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="GoogleCreativeNoConvReport" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="GoogleContentReport" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="ReportTypes" type="Easynet.Edge.Services.DataRetrieval.Configuration.ReportTypesElementSection, Edge.Services.DataRetrieval" />
		<section name="GatewayName" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="BackOfficeTable" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="PPCCreativeTable" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="PPCContentTable" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="GeneralBackOffice" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="YolaBackOffice" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="PcSpeedBackOffice" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="oovooBackOffice" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="DirectBackOffice" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="FacebookRetriever" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection,  Edge.Services.DataRetrieval" />
		<section name="EasyForexBackOffice" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="SpotOptionBackOffice" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="ConduitBackOffice" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="RadvisionBackOffice" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="BabylonBackOffice" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="iOptionBackOffice" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="HarmonIeBackOffice" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="ForexAffiliateBackOffice" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
		<section name="KampyleBackOffice" type="Easynet.Edge.Services.DataRetrieval.Configuration.FieldElementSection, Edge.Services.DataRetrieval" />
	</configSections>

	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<probing privatePath="Services" />
		</assemblyBinding>
	</runtime>

	<!-- ======================================================== -->
	<!--						App Settings					    -->
	<!-- ======================================================== -->
	<appSettings>
		<add key="DebugMode" value="true" />

		<!--Core-->
		<add key="Easynet.Edge.Core.Data.DataManager.Connection.String" value="Data Source=localhost;Initial Catalog=Seperia;Integrated Security=false;User ID=edge;PWD=edgebi!" />
		<add key="Easynet.Edge.Core.Data.DataManager.Connection.Timeout" value="200" />
		<add key="Easynet.Edge.Core.Services.Service.BaseListeningUrl" value="net.tcp://localhost:3535/{0}"/>
		<add key="Easynet.Edge.Core.Services.Service.AssemblyDirectory" value="Services"/>
		<add key="Easynet.Edge.Core.Configuration.ServiceElement.DefaultMaxExecutionTime" value="00:40:00"/>
		<add key="Easynet.Edge.Core.Utilities.Log.LogName" value="easynet Edge"/>
		<add key="Easynet.Edge.Core.Domain" value="seperia.hz"/>

		<!--ScheduleManagement-->
		<add key="Easynet.Edge.Services.ScheduleManagement.ScheduleBuilder.ResultsRoot" value="D:\Edge\Seperia Retrieved Files" />
		<add key="Easynet.Edge.Services.ScheduleManagement.ScheduleTable.KeepAliveCheckTime" value="5" />
		<add key="MyScheduler.Scheduler.Percentile" value="80"/>
		<add key="MyScheduler.Scheduler.NeededScheduleTimeLine" value="02:00:00"/>
		<add key="MyScheduler.Scheduler.IntervalBetweenNewSchedule" value="00:10"/>
		<add key="MyScheduler.Scheduler.FindServicesToRunInterval" value="00:01"/>
		<add key="MyScheduler.Scheduler.DeleteEndedServiceInterval" value="24:00:00"/>

		<!--BaseService-->
		<add key="Easynet.Edge.Services.DataRetrieval.BaseService.SourceConnectionString" value="Data Source=localhost;Initial Catalog=Source;Integrated Security=false;User ID=edge;PWD=edgebi!" />
		<add key="Easynet.Edge.Services.DataRetrieval.BaseService.ResultsRoot" value="D:\Edge\Seperia Retrieved Files" />

		<!--RetrieverService-->
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.RetrieverService.UserAgent" value="Mozilla/5.0 (Windows; U; Windows NT 6.0; en-US; rv:1.8.1.3) Gecko/20070309 Firefox/2.0.0.3" />
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.RetrieverService.FileExtension" value="xml" />
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.RetrieverService.RequestTimeout" value="0:05:00" />
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.RetrieverService.ReadTimeout" value="0:05:00" />

		<!--Adwords-->
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.AdWordsRetriever.MaxRetries" value="2" />
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.AdWordsRetriever.MaxInstancesReRuns" value="31" />
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.AdWordsRetriever.PrecentDiffrence" value="1" />
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.AdWordsRetriever.TargetSubDirectory" value="Google\Adwords\" />
		<add key="Easynet.Edge.Services.DataRetrieval.AdWordsCreativeProcessor.ErrorSubDirectory" value="Google\Creative\Errors\" />
		<add key="Easynet.Edge.Services.DataRetrieval.AdWordsCreativeProcessor.FileExtension" value="txt" />
		<add key="Easynet.Edge.Services.DataRetrieval.AdwordsContentProcessor.ErrorSubDirectory" value="Google\Content\Errors\" />
		<add key="Easynet.Edge.Services.DataRetrieval.AdwordsContentProcessor.FileExtension" value="txt" />

		<!--BackOffice Retriever-->
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.BackOfficeGenericRetriever.MaxRetries" value="2" />
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.BackOfficeGenericRetriever.MaxInstancesReRuns" value="31" />
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.BackOfficeGenericRetriever.PrecentDiffrence" value="1" />
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.BackOfficeGenericRetriever.TargetSubDirectory" value="BackOffice\" />

		<!--Babylon BackOffice Retriever-->
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.BabylonBackOfficeRetreiver.MaxRetries" value="2" />
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.BabylonBackOfficeRetreiver.MaxInstancesReRuns" value="31" />
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.BabylonBackOfficeRetreiver.PrecentDiffrence" value="1" />
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.BabylonBackOfficeRetreiver.TargetSubDirectory" value="BackOffice\" />

		<!--BackOffice Processor-->
		<add key="Easynet.Edge.Services.DataRetrieval.Processor.BackOfficeProcessor.ErrorSubDirectory" value="BackOffice\Errors\" />
		<add key="Easynet.Edge.Services.DataRetrieval.Processor.BackOfficeProcessor.FileExtension" value="txt" />
		<add key="Easynet.Edge.Services.DataRetrieval.Processor.BackOfficeProcessorNodes.ErrorSubDirectory" value="BackOffice\Errors\" />
		<add key="Easynet.Edge.Services.DataRetrieval.Processor.BackOfficeProcessorNodes.FileExtension" value="txt" />

		<!--Importer-->
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.ImporterRetriever.TargetSubDirectory" value="Seperia IMPORT\PPC\" />
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.ImporterRetriever.FileExtension" value="txt" />
		<add key="Easynet.Edge.Services.DataRetrieval.Processor.ImporterProcessor.ErrorSubDirectory" value="Seperia IMPORT\Errors\" />
		<add key="Easynet.Edge.Services.DataRetrieval.Processor.ImporterProcessor.FileExtension" value="txt" />

		<!--Organic-->
		<add key="Easynet.Edge.Services.Google.OrganicRankings.RetrieverService.MaxInstancesReRuns" value="31" />
		<add key="Easynet.Edge.Services.Google.OrganicRankings.RetrieverService.PrecentDiffrence" value="0.8" />
		<add key="Easynet.Edge.Services.Google.OrganicRankings.RetrieverService.UrlFormat" value="http://www.google.com/custom?q={0}&amp;ud=1&amp;num={1}&amp;start={2}&amp;gl=il&amp;hl=iw&amp;ie=utf8&amp;oe=utf8&amp;output=xml_no_dtd&amp;client=google-csbe&amp;cx=!001690554566158736806%3Asc0vx0qcmus" />
		<add key="Easynet.Edge.Services.Google.OrganicRankings.RetrieverService.TargetSubDirectory" value="Google\OrganicRankings\" />

		<!-- Easy Forex -->
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.EasyForexBackOfficeRetriever.TargetSubDirectory" value="BackOffice\"/>
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.EasyForexBackOfficeRetriever.MaxRetries" value="2" />
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.EasyForexBackOfficeRetriever.MaxInstancesReRuns" value="31" />
		<add key="Easynet.Edge.Services.DataRetrieval.Retriever.EasyForexBackOfficeRetriever.PrecentDiffrence" value="1" />
		<add key="Easynet.Edge.Services.BackOffice.EasyForex.EasyForexBackOfficeRetrieverService.TargetSubDirectory" value="BackOffice\" />

		<!--Messaging-->
		<add key="Easynet.Edge.Messaging.Host" value="mailgw.netvision.net.il"/>
		<add key="Easynet.Edge.Messaging.Port" value="25"/>

		<!--Facebook Retriever-->
		<add key="Easynet.Edge.Services.Facebook.FacebookBaseRetriever.ResultOutputPath" value="D:\Edge\Retrieved Files\Facebook\Xml\" />
		<add key="Easynet.Edge.Services.Facebook.FacebookRetriever.ErrorSubDirectory" value="creative\Errors\" />
		<add key="Easynet.Edge.Services.Facebook.FacebookRetriever.FileExtension" value="txt" />
		<add key="Easynet.Edge.Services.Facebook.FacebookRetriever.MaxRetries" value="2" />
		<add key="Easynet.Edge.Services.Facebook.FacebookRetriever.MaxInstancesReRuns" value="31" />
		<add key="Easynet.Edge.Services.Facebook.FacebookRetriever.PrecentDiffrence" value="1" />
		<add key="Easynet.Edge.Services.Facebook.FacebookRetriever.ResultsRoot" value="D:\Edge\" />
		<add key="ClientSettingsProvider.ServiceUri" value="" />
	</appSettings>

	<edge.services>

		<!-- ============ Extensions ============ -->
		<Extensions>
			<Extension Name="Directories" Type="Easynet.Edge.Services.FileImport.DirectoryElementCollection, Edge.Services.FileImport"/>
		</Extensions>

		<!-- ======================================================== -->
		<!--						Services						  -->
		<!-- ======================================================== -->
		<Services>

			<!-- ======================================================== -->
			<!--					EdgeServiceHost services			    -->
			<!-- ======================================================== -->
			<Service Name="ScheduleManagerSeperia"
						   Class="Easynet.Edge.Services.ScheduleManagement.ScheduleManagerService, Edge.Services.ScheduleManagement" />

			<!-- ======================================================== -->
			<!--					System Services						    -->
			<!-- ======================================================== -->

			<!-- FileSystemWatcher -->
			<Service Name="FileSystemWatcher"
						   Class="Easynet.Edge.Services.FileImport.FileSystemWatcherService, Edge.Services.FileImport">
				<!--MaxInstances="1"-->

				<Directories>
					<Directory Path="D:\Edge\Seperia IMPORT\PPC\Creative" Filter="*.txt" HandlerService="PpcCreativeImporter"/>
					<Directory Path="D:\Edge\Seperia IMPORT\BackOffice\Babylon" Filter="*.txt" HandlerService="BabylonBOImporter"/>
					<Directory Path="D:\Edge\Seperia IMPORT\Refund\Creative" Filter="*.txt" HandlerService="RefundPpcCreativeImporter"/>
					<Directory Path="D:\Edge\Seperia IMPORT\Bing\PPC\Creative" Filter="*.txt" HandlerService="BingPpcCreativeImporter"/>
					<Directory Path="D:\Edge\Seperia IMPORT\PPC\Content" Filter="*.txt" HandlerService="PpcContentImporter"/>
					<Directory Path="D:\Edge\Seperia IMPORT\Yahoo\Easyforex" Filter="*.txt" HandlerService="EFPpcCreativeImporter"/>
					<Directory Path="D:\Edge\Seperia IMPORT\BackOffice\EyeBuyDirect" Filter="*.txt" HandlerService="EyeBuyDirectBOImporter"/>
					<Directory Path="D:\Edge\Seperia IMPORT\BackOffice\Yola" Filter="*.txt" HandlerService="YolaDirectBOImporter"/>
					<Directory Path="D:\Edge\Seperia IMPORT\BackOffice\PcSpeed" Filter="*.txt" HandlerService="PcSpeedDirectBOImporter"/>
					<Directory Path="D:\Edge\Seperia IMPORT\BackOffice\OOVOO" Filter="*.txt" HandlerService="oovooDirectBOImporter"/>
					<Directory Path="D:\Edge\Seperia IMPORT\BackOffice\Direct" Filter="*.txt" HandlerService="DirectBOImporter"/>
					<Directory Path="D:\Edge\Seperia IMPORT\BackOffice\EasyForex" Filter="*.txt" HandlerService="EasyForexBOImporter"/>
				</Directories>

				<SchedulingRules>
					<Rule CalendarUnit="AlwaysOn" />
				</SchedulingRules>
			</Service>

			<!-- ======================================================== -->
			<!--						Delete days						    -->
			<!-- ======================================================== -->

			<Service Name ="GoogleDeleteDays"
						  Class="Easynet.Edge.Services.Utilities.ExecuteStoredProcedureService,Edge.Services.Utilities.ExecuteStoredProcedureService"
						  ConnectionTimeout="02:30:00"
						  Procedure="Sp_Delete_Table_Days_WithDefaultDayCode(@Account_ID:NVarchar, @day:NVarchar, @Table_Name:NVarchar, @Channel_ID:NVarchar)"
						  param.Table_Name="Paid_API_AllColumns"
						  param.Account_ID="{../AccountID}"
						  param.day="{../Configuration.Options.Date}"
						  param.Channel_ID="1">
			</Service>

			<Service Name ="ContentDeleteDays"
						  Class="Easynet.Edge.Services.Utilities.ExecuteStoredProcedureService,Edge.Services.Utilities.ExecuteStoredProcedureService"
						  ConnectionTimeout="02:30:00"
						  Procedure="Sp_Delete_Table_Days_WithDefaultDayCode(@Account_ID:NVarchar, @day:NVarchar, @Table_Name:NVarchar, @Channel_ID:NVarchar)"
						  param.Table_Name="Paid_API_Content"
						  param.Account_ID="{../AccountID}"
						  param.day="{../Configuration.Options.Date}"
						  param.Channel_ID="1">
			</Service>

			<Service Name ="BackOfficeDeleteDays"
						  Class="Easynet.Edge.Services.Utilities.ExecuteStoredProcedureService,Edge.Services.Utilities.ExecuteStoredProcedureService"
						  ConnectionTimeout="02:30:00"
						  Procedure="Sp_Delete_Table_Days_no_channel_WithDefaultDayCode(@Account_ID:NVarchar, @day:NVarchar, @Table_Name:NVarchar)"
						  param.Table_Name="Paid_API_AllColumns"
						  param.Account_ID="{../AccountID}"
						  param.day="{../Configuration.Options.Date}">
			</Service>

			<Service Name="DeleteDays"
						   AggregationType="day"
						   MaxInstances="1" >
				<ExecutionSteps>
					<Step ServiceToUse="GoogleDeleteDays"/>
				</ExecutionSteps>
			</Service>

			<Service Name ="FacebookDeleteDays"
					 
						  Class="Easynet.Edge.Services.Utilities.ExecuteStoredProcedureService,Edge.Services.Utilities.ExecuteStoredProcedureService"
						  ConnectionTimeout="0:10:00"
						  Procedure="Sp_Delete_Table_Days(@Account_ID:NVarchar, @day:NVarchar, @Table_Name:NVarchar, @Channel_ID:NVarchar)"
						  param.Table_Name="Paid_API_AllColumns"
						  param.Account_ID="{../AccountID}"
						  param.day="{../TimeScheduled}"
						  param.Channel_ID="6">
			</Service>


			<!-- ======================================================== -->
			<!--						PpcImport						    -->
			<!-- ======================================================== -->

			<!--Seperia BOImporter-->
			<Service Name="EasyForexBOImporter">
				<ExecutionSteps>
					<Step ServiceToUse="ImporterRetriever"
								  TargetDirectory="Seperia Import\BackOffice\EasyForex\"/>
					<Step ServiceToUse="ImporterProcessor"
								  BackOfficeFile="True" TableMapping="BackOfficeTable" FieldsMapping="EasyForexBackOffice" TableName="BackOffice_Client_Gateway"/>
				</ExecutionSteps>
			</Service>

			<Service Name="BabylonBOImporter">
				<ExecutionSteps>
					<Step ServiceToUse="ImporterRetriever"
									TargetDirectory="Seperia Import\BackOffice\Babylon\"/>
					<Step ServiceToUse="ImporterProcessor"
									BackOfficeFile="True" TableMapping="BackOfficeTable" FieldsMapping="BabylonBackOffice" TableName="BackOffice_Client_Gateway"/>
				</ExecutionSteps>
			</Service>

			<Service Name="PpcCreativeImporter"
						   MaxInstances="1">
				<ExecutionSteps>
					<Step ServiceToUse="ImporterRetriever"
									TargetDirectory="Seperia Import\PPC\Creative\"/>
					<Step ServiceToUse="ImporterProcessor"
									ReportType ="Creative" TableMapping="PPCCreativeTable" FieldsMapping="GoogleCreativeReport" TableName="Paid_API_AllColumns"/>
				</ExecutionSteps>
			</Service>

			<Service Name="RefundPpcCreativeImporter"
						  MaxInstances="1"
						  ShouldDeleteDay ="false">
				<ExecutionSteps>
					<Step ServiceToUse="ImporterRetriever"
									TargetDirectory="Seperia Import\PPC\Creative\"/>
					<Step ServiceToUse="ImporterProcessor"
									 ReportType ="Creative" ShouldDeleteDay ="false" TableMapping="PPCCreativeTable" FieldsMapping="GoogleCreativeReport" TableName="Paid_API_AllColumns"/>
				</ExecutionSteps>
			</Service>

			<Service Name="EFPpcCreativeImporter"
				   MaxInstances="1">
				<ExecutionSteps>
					<Step ServiceToUse="ImporterRetriever"
									TargetDirectory="Seperia Import\PPC\Creative\"/>
					<Step ServiceToUse="ImporterProcessor" TrackingParameters="gid=,false|gtw/,false"
									ReportType ="Creative" TableMapping="PPCCreativeTable" FieldsMapping="GoogleCreativeReport" TableName="Paid_API_AllColumns"/>
				</ExecutionSteps>
			</Service>

			<Service Name="EyeBuyDirectBOImporter">
				<ExecutionSteps>
					<Step ServiceToUse="ImporterRetriever"
									TargetDirectory="Import\BackOffice\EyeBuyDirect\"/>
					<Step ServiceToUse="ImporterProcessor"
									BackOfficeFile="True" TableMapping="BackOfficeTable" FieldsMapping="GeneralBackOffice" TableName="BackOffice_Client_Gateway"/>
				</ExecutionSteps>
			</Service>

			<Service Name="YolaDirectBOImporter">
				<ExecutionSteps>
					<Step ServiceToUse="ImporterRetriever"
									TargetDirectory="Import\BackOffice\Yola\"/>
					<Step ServiceToUse="ImporterProcessor"
									BackOfficeFile="True" TableMapping="BackOfficeTable" FieldsMapping="YolaBackOffice" TableName="BackOffice_Client_Gateway"/>
				</ExecutionSteps>
			</Service>

			<Service Name="PcSpeedDirectBOImporter">
				<ExecutionSteps>
					<Step ServiceToUse="ImporterRetriever"
									TargetDirectory="Import\BackOffice\PcSpeed\"/>
					<Step ServiceToUse="ImporterProcessor"
									BackOfficeFile="True" TableMapping="BackOfficeTable" FieldsMapping="PcSpeedBackOffice" TableName="BackOffice_Client_Gateway"/>
				</ExecutionSteps>
			</Service>

			<Service Name="oovooDirectBOImporter">
				<ExecutionSteps>
					<Step ServiceToUse="ImporterRetriever"
									TargetDirectory="Import\BackOffice\oovoo\"/>
					<Step ServiceToUse="ImporterProcessor"
									BackOfficeFile="True" TableMapping="BackOfficeTable" FieldsMapping="oovooBackOffice" TableName="BackOffice_Client_Gateway"/>
				</ExecutionSteps>
			</Service>

			<Service Name="DirectBOImporter">
				<ExecutionSteps>
					<Step ServiceToUse="ImporterRetriever"
									TargetDirectory="Import\BackOffice\Direct\"/>
					<Step ServiceToUse="ImporterProcessor"
									BackOfficeFile="True" TableMapping="BackOfficeTable" FieldsMapping="DirectBackOffice" TableName="BackOffice_Client_Gateway"/>
				</ExecutionSteps>
			</Service>

			<Service Name="BingPpcCreativeImporter">
				<ExecutionSteps>
					<Step ServiceToUse="ImporterRetriever"
									TargetDirectory="Seperia Import\PPC\Creative\"/>
					<Step ServiceToUse="ImporterProcessor"
									ReportType ="Creative" TableMapping="PPCCreativeTable" FieldsMapping="BingCreativeReport" TableName="Paid_API_AllColumns"/>
				</ExecutionSteps>
			</Service>

			<Service Name="PpcContentImporter">
				<ExecutionSteps>
					<Step ServiceToUse="ImporterRetriever"
									TargetDirectory="Seperia Import\PPC\Content\"/>
					<Step ServiceToUse="ImporterProcessor" TableMapping="PPCContentTable" FieldsMapping="GoogleContentReport" TableName="Paid_API_Content"
									ReportType ="Content"/>
				</ExecutionSteps>
			</Service>

			<Service Name="GoogleAdwordsImoprter">
				<ExecutionSteps>
					<Step ServiceToUse="ImporterRetriever"
									TargetDirectory="Seperia Import\Google\Adwords\"/>
					<Step ServiceToUse="AdWordsCreativeProcessor" IsBlocking="false" MaxExecutionTime="1:30" TableMapping="PPCCreativeTable" FieldsMapping="GoogleCreativeReport"  />
				</ExecutionSteps>
			</Service>

			<Service Name="ImporterRetriever"
						  Class="Easynet.Edge.Services.DataRetrieval.Retriever.ImporterRetriever, Edge.Services.DataRetrieval">
			</Service>

			<Service Name="ImporterProcessor"
						   Class="Easynet.Edge.Services.DataRetrieval.Processor.ImporterProcessor, Edge.Services.DataRetrieval"
						   Readertype="Easynet.Edge.Services.DataRetrieval.DataReader.CsvReader, Edge.Services.DataRetrieval"/>


			<!-- ======================================================== -->
			<!--						AdWords							  -->
			<!-- ======================================================== -->


			<!-- AdWordsContentProcessor -->
			<Service Name="AdWordsContentProcessor"
						   Class="Easynet.Edge.Services.DataRetrieval.Processor.AdwordsContentProcessor, Edge.Services.DataRetrieval"
						   ChannelID="1"
						   RowName="row"
						   Readertype="Easynet.Edge.Services.DataRetrieval.DataReader.XmlAttributesReader, Edge.Services.DataRetrieval"/>

			<!-- AdWordsCreativeProcessor -->
			<Service Name="AdWordsCreativeProcessor"
						   Class="Easynet.Edge.Services.DataRetrieval.Processor.AdwordsCreativeProcessor, Edge.Services.DataRetrieval"
						   ChannelID="1"
						   RowName="row"
						   Readertype="Easynet.Edge.Services.DataRetrieval.DataReader.XmlAttributesReader, Edge.Services.DataRetrieval"/>

			<Service Name="AdWordsContent"
						   AggregationType="day"
						   OffsetDays ="-3"
						   MaxInstances="1">
				<ExecutionSteps>
					<Step ServiceToUse="AdWordsRetrieverAccounts" IsBlocking="true" FailureRepeat="3" MaxExecutionTime="1:30" ReportType ="Content" />
					<Step ServiceToUse="AdWordsContentProcessor" IsBlocking="false" MaxExecutionTime="1:30" TableMapping="PPCContentTable" FieldsMapping="GoogleContentReport" />
				</ExecutionSteps>
				<SchedulingRules>
					<Rule ExactTimes="15:00" CalendarUnit="Day" MaxDeviation="0:10" />
				</SchedulingRules>
			</Service>

			<Service Name="AdWordsContentAccount"
			   AggregationType="day"
			   OffsetDays ="-3"
			   MaxInstances="1">
				<ExecutionSteps>
					<Step ServiceToUse="AdWordsRetrieverAccounts" IsBlocking="true" FailureRepeat="3" MaxExecutionTime="1:30" ReportType ="Content" />
					<Step ServiceToUse="AdWordsContentProcessor" IsBlocking="false" MaxExecutionTime="1:30" TableMapping="PPCContentTable" FieldsMapping="GoogleContentReport" />
				</ExecutionSteps>
			</Service>

			<Service Name="AdWordsContentDeleteFalse"
						   AggregationType="day"
						   OffsetDays ="-3"
						   MaxInstances="1"
