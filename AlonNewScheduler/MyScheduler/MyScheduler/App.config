				<ExecutionSteps>
					<Step ServiceToUse="AdWordsRetriever" IsBlocking="true" FailureRepeat="3" MaxExecutionTime="8:30" ReportType ="Content" />
					<Step ServiceToUse="AdWordsContentProcessor" IsBlocking="false" MaxExecutionTime="8:30" TableMapping="PPCContentTable" FieldsMapping="GoogleContentReport" />
				</ExecutionSteps>
			</Service>			

			<Service Name="FacebookCreative" MaxInstancesPerAccount="1" MaxInstances="1">
