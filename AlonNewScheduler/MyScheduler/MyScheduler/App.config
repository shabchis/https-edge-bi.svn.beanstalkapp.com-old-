		<Services>			
			<Service Name="FileSystemWatcher" MaxInstancesPerAccount="1" MaxInstances="1"
					 Class="Easynet.Edge.Services.FileImport.FileSystemWatcherService, Edge.Services.FileImport">				
				<SchedulingRules>
					<Rule CalendarUnit="AlwaysOn" />
				</SchedulingRules>
			</Service>

			<Service Name="ScheduleBuildingCue" MaxInstancesPerAccount="1" MaxInstances="1"
					 Class="Easynet.Edge.Services.ScheduleManagement.ScheduleBuildingCueService, Edge.Services.ScheduleManagement">
				<SchedulingRules>
					<Rule ExactTimes="00:05" CalendarUnit="Day" MaxDeviation="0:10" NextDay="True"/>
				</SchedulingRules>
			</Service>			
		
			<Service Name="AdWordsContent"
					 AggregationType="day"
					 OffsetDays ="-3"
					 MaxInstancesPerAccount="1" MaxInstances="1">

				<ExecutionSteps>
					<Step name ="AdWordsRetriever" ServiceToUse="AdWordsRetriever" IsBlocking="true" FailureRepeat="3" MaxExecutionTime="0:30" ReportType ="Content" />
					<Step name="AdWordsCreativeProcessor" ServiceToUse="AdWordsContentProcessor" IsBlocking="false" MaxExecutionTime="0:30" TableMapping="PPCContentTable" FieldsMapping="GoogleContentReport" />
				</ExecutionSteps>
				<!--<SchedulingRules>
					<Rule ExactTimes="17:00" CalendarUnit="Day" MaxDeviation="3:45"  />
				</SchedulingRules>-->
			</Service>
		
			<Service Name="AdWordsCreative"
					 AggregationType="day"
					 MaxInstancesPerAccount="1" MaxInstances="1">
				<ExecutionSteps>
					<Step name ="AdWordsRetriever" ServiceToUse="AdWordsRetriever" IsBlocking="true" FailureRepeat="3" MaxExecutionTime="1:00"  ReportType ="Creative"/>
					<Step name="AdWordsCreativeProcessor" ServiceToUse="AdWordsCreativeProcessor" IsBlocking="false" MaxExecutionTime="1:30" TableMapping="PPCCreativeTable" FieldsMapping="GoogleCreativeReport" />
				</ExecutionSteps>
				<!--<SchedulingRules>
					<Rule ExactTimes="0:30" CalendarUnit="Day" MaxDeviation="6:15" />
				</SchedulingRules>-->
			</Service>

			<Service Name="AdWordsRetriever" MaxInstancesPerAccount="1" MaxInstances="1"
						   Class="Easynet.Edge.Services.DataRetrieval.Extensions.CampaignFilteringService, Edge.Services.DataRetrieval.Extensions"
				 ChannelID="1"
					 />
			
