					<Step ServiceToUse="AdWordsCreativeProcessor" IsBlocking="false" MaxExecutionTime="8:30" TableMapping="PPCCreativeTable" FieldsMapping="GoogleCreativeReport"/>
				</ExecutionSteps>
			</Service>
			
			<Service Name="AdWordsContentReRun"
					 AggregationType="day"
